    "includeScheduled": {
      "type": "boolean",
      "default": true
    },
    "includeManual": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "patrons-tips",
  "editorScript": "file:index.js",
  "editorStyle": "file:index.css",
  "style": "file:style-index.css",
  "script": [
